  "scripts": {
    "puppeteer": "find / -type f -name 'puppeteer'",
    "chromium": "find / -type f -name 'chromium-browser'",
    "chrome": "find / -type f -name 'chromium' 2>/dev/null || echo 'Chromium not found'",
    "path": "echo $PATH",
    "find": "yarn puppeteer && yarn chromium && yarn chrome && yarn path"
  },
  "dependencies": {
    "puppeteer": "19.7.1"
  }
}
